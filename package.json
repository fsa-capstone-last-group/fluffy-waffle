    "babel-loader": "^9.1.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "node-loader": "^2.0.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  }
